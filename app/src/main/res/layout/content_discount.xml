        app:layout_constraintEnd_toEndOf="@+id/nav_host_fragment_content_discount"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nav_host_fragment_content_discount"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/discountHeaderTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/discount_header_two"
